package CUNSTRUCTOR_CHAINING;

/**
 * @author :IRFAN
 *TIME:3:26:46 pm
 * DATE:06-Jun-2017
 *PROJECTNAME:6-6-17WORKING-ON-CUNSTRUCTOR
 */
public class CUNSTRUCTORCHAINING extends superchaining{
	
public	CUNSTRUCTORCHAINING(){
	
	//we call one paramitrized cunstructor from herre
	super(1);
	System.out.println("0 paramitrized cunstructor");
}
public	CUNSTRUCTORCHAINING(int a){
	this(1,3);
	System.out.println("from one paramitrizd constructor");
}
public	CUNSTRUCTORCHAINING(int a,int b){
	this(1,2,3);
	System.out.println("from second paramitrized cunstructor");
	
}
public	CUNSTRUCTORCHAINING(int a,int b,int c){
	System.out.println("from three argument cunstructor");
}
public static void main(String[] args) {
	CUNSTRUCTORCHAINING c=new CUNSTRUCTORCHAINING();
	
	
}
}
