public class palindromString {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String str="irfan";
		String rev="";
		for(int i=str.length();i>0;--i){
			rev+=str.charAt(i);
		}
		System.out.println(rev);

	}

}
